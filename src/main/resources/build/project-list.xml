<?xml version="1.0"?>

<!-- ======================================================================= -->
<!-- Handpay back server build file                                                       -->
<!-- ======================================================================= -->

<project name="project name list" default="" basedir=".">

	<property file="project-list.properties"/>
	<property environment="env" />
	
	<path id="svnant.classpath">
    	<fileset dir="lib/svnant-1.3.0">
		  <include name="*.jar"/>
		</fileset>
    </path> 
	<path id="contrib.classpath">
    	<fileset dir="lib/ant-contrib">
		  <include name="*.jar"/>
		</fileset>
    </path> 
	
	<!-- load the svnant task -->
    <typedef resource="org/tigris/subversion/svnant/svnantlib.xml" classpathref="svnant.classpath"/>
	
	<!-- load the contrib task -->
    <taskdef resource="net/sf/antcontrib/antlib.xml"  classpathref="contrib.classpath"/> 
	
	<propertyselector property="project.list" delimiter="," match="project\.name\.([^\.]*)" select="\1" casesensitive="true"/>
	
	<propertyselector property="relation.list" delimiter="," match="relation\.name\.([^\.]*)" select="\1" casesensitive="true"/>
