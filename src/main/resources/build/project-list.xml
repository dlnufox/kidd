	<propertyselector property="prop.test.list" delimiter="," match="prop\.test\.([^\.]*)" select="\1" casesensitive="true"/>
	<propertyselector property="prop.online.list" delimiter="," match="prop\.online\.([^\.]*)" select="\1" casesensitive="true"/>
	<propertyselector property="prop.back.list" delimiter="," match="prop\.back\.([^\.]*)" select="\1" casesensitive="true"/>
	
	<propertyselector property="project.package.list" delimiter="," match="project\.cp\.([^\.]*)" select="\1" casesensitive="true"/>
	-->
	
</project>

